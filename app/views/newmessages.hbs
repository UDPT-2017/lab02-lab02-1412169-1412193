<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">New Message</h3>
  </div>
  <div class="panel-body">
    <form action="/newmessage" method="post" onsubmit="return CheckMessage();">
      <div class="alert alert-danger" role="alert" style="display:none;" id="error-inf">
          <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
          <span class="sr-only" id="Common-err"></span>
      </div>
      <div class="form-group">
        <!-- se chua select-->
        <select name="EmailID" class="form-control" id="sel1">
          {{#if list.length}}
              {{#each list}}
                <option value="{{this.id}}">{{this.email}}</option>
              {{/each}}
          {{/if}}
        </select>
      </div>
      <div class="form-group">
        <input name="fchude" type="text" class="form-control" id="chudeEMAIL" placeholder="Chủ đề">
      </div>
      <div class="form-group">
        <textarea name="fnoidung" id="noidung" rows="8" cols="80" class="form-control" placeholder="Nội dung"></textarea>
      </div>
      <button type="submit" class="btn btn-primary">Sent</button>
    </form>
  </div>
</div>
<script src="/js/newmessage.js"></script>
